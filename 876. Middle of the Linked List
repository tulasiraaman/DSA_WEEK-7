class Solution {
    public ListNode middleNode(ListNode head) {
        ListNode slow = head;
        ListNode fast = head;

        while(fast != null && fast.next != null){ //fast = even num, fast.next = odd num
            slow = slow.next;
            fast = fast.next.next;
        }
        return slow;        
    }//TC - O(n), SC - O(1)
    //Heir and Turtle algorithm
}
